<h1>Badges</h1>
<table class="table">
  <thead>
    <tr>
      <th scope="col">Name</th>
      <th scope="col">Created at</th>
      <th scope="col">Updated at</th>
      <th></th>
    </tr>
  </thead>
  <tbody>
    <% @badges.each do |badge| %>
      <tr>
        <td><%= badge.name %></td>
        <td><%= badge.created_at %></td>
        <td><%= badge.updated_at %></td>
        <td><%= link_to "Show", badge %></td>
      </tr>
    <% end %>
  </tbody>
</table>
